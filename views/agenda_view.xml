<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="agenda_view_form" model="ir.ui.view">
			<field name="name">Agenda From</field>
			<field name="model">agenda</field>
			<field name="arch" type="xml">
				<form string="agenda">
					<sheet>
						<group name="agenda_form">
							<field name="nombre"/>
							<field name="apellidos"/>
							<field name="telefono"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="agenda_view_tree" model="ir.ui.view">
			<field name="name">Agenda Tree</field>
			<field name="model">agenda</field>
			<field name="arch" type="xml">
				<tree>
					<field name="nombre"/>
					<field name="apellidos"/>
					<field name="telefono"/>
				</tree>
			</field>
		</record>
		<act_window
 id="action_agenda"
 name="agenda"
 res_model="agenda"
 view_mode="tree,form"
 />
		<menuitem id="menu_agenda_agenda" name="agenda"
action="action_agenda"/>
	</data>
</odoo>